inherits  b2JointDef

public target as variant= new b2Vec2()
public maxForce as variant= null
public frequencyHz as variant= null
public dampingRatio as variant= null
public timeStep as variant= null

public  sub _new()

		' //  The constructor for b2JointDef
		 me.type = b2Joint.e_unknownJoint
		 me.userData = null
		 me.body1 = null
		 me.body2 = null
		 me.collideConnected = false
		' // 
		' //  initialize instance variables for references
		 me.target = new b2Vec2()
		' // 
		 me.type = b2Joint.e_mouseJoint
		 me.maxForce = 0.0
		 me.frequencyHz = 5.0
		 me.dampingRatio = 0.7
		 me.timeStep = 1.0 / 60.0
end


